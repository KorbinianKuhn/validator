{
  "extends": ["eslint:recommended", "plugin:ava/recommended", "prettier"],
  "rules": {
    "prettier/prettier": ["error", {
      "singleQuote": true
    }],
    "no-console": "off"
  },
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module"
  },
  "parser": "babel-eslint",
  "plugins": ["ava", "prettier"],
  "env": {
    "node": true
  }
}
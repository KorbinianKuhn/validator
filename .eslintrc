{
  "extends": ["eslint:recommended", "prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "no-console": "off",
    "require-atomic-updates": "off"
  },
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module"
  },
  "parser": "babel-eslint",
  "plugins": ["prettier"],
  "env": {
    "jest": true,
    "node": true,
    "es6": true
  }
}

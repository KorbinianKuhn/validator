{
  "name": "@korbiniankuhn/validator",
  "version": "0.1.7",
  "description": "Validate input parameters for expressjs, Angular and mongoose.",
  "main": "index.js",
  "scripts": {
    "test": "mocha \"./test/**/*.spec.js\"",
    "test:coverage": "nyc --reporter=html mocha \"./test/**/*.spec.js\"",
    "test:coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KorbinianKuhn/validator.git"
  },
  "keywords": [
    "express",
    "validator",
    "validation",
    "security",
    "parser",
    "async",
    "await",
    "promise",
    "angular",
    "mongoose"
  ],
  "author": "Korbinian Kuhn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KorbinianKuhn/validator/issues"
  },
  "homepage": "https://github.com/KorbinianKuhn/validator#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-lodash": "^3.3.2",
    "babel-preset-env": "^1.6.1",
    "coveralls": "^3.0.0",
    "eslint": "^4.18.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0",
    "lodash-webpack-plugin": "^0.11.5",
    "mocha": "^5.0.5",
    "nyc": "^11.4.1",
    "pre-commit": "^1.2.2",
    "prettier": "^1.11.1",
    "should": "^13.2.1",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.14"
  },
  "dependencies": {
    "lodash": "^4.17.5",
    "moment": "^2.20.1"
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "pre-commit": [
    "lint-staged"
  ]
}
